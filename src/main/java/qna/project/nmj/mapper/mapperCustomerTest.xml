<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="qna.project.nmj.beans.dao.CustomerTestDAO">
	
	<!-- 마이페이지 - 회원 정보 수정 -->
	<select id="selectMemberByUid" resultType="qna.project.nmj.beans.MemberDTO">
		SELECT mb_img_sav, mb_tel, mb_name, mb_id, mb_pw, mb_email
		FROM NMJ_member
		WHERE mb_uid = #{mb_uid};
	</select>
	
	<!-- 마이페이지 - 예약/찜 목록보기 -->
		
	<!-- 마이페이지 - 내 리뷰보기 -->
	
</mapper>